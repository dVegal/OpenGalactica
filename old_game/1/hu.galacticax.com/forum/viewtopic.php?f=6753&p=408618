<script type="text/javascript">
$(document).ready(function(){
    //for table row
    $(".inner:even").css("background-color", "#212121");
    $(".inner:even .21").css("display", "");
    $(".inner:odd").css("background-color", "#1c1c1c");
    $(".inner:odd .1c").css("display", "");

    $(".author .1c").css("display", "");

    //$(".post_row .corners-bottom").hide();

    $(".post_row").bind("mouseenter", function(){
        $("#"+this.id+" .corners-bottom").show();
    });

    $(".post_row").bind("mouseleave", function(){
        $("#"+this.id+" .corners-bottom").hide();
    });
});
</script>

