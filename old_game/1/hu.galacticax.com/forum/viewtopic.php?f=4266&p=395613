<script type="text/javascript">
            function get_width(id, rand_num)
            {
                var div_planet_name = document.getElementById('planet_name_div_'+id+'_'+rand_num);
                var takaro_div = document.getElementById('takaro_'+id+'_'+rand_num);
                var flag_div = document.getElementById('flag_'+id+'_'+rand_num);
                var alliance_div = document.getElementById('alliance_'+id+'_'+rand_num);

                var width = div_planet_name.offsetWidth;

                if (width > 90) {
                    div_planet_name.style.width = '90px';
                    takaro_div.style.visibility = 'visible';
                    takaro_div.style.left = '146px';
                }
                var width_new = div_planet_name.offsetWidth;

                flag_div.style.left = (64+width_new+12)+'px';
                alliance_div.style.left = (64+width_new+12+23)+'px';
            }
         </script>
