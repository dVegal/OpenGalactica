<script type="text/javascript">
  function refresh()
  {
     var pos = document.location.href.indexOf('#');
     if ( pos > -1 ) document.location.href=document.location.href.substr(0, document.location.href.indexOf('#'));
     else document.location.href=document.location.href;
  };
</script>

