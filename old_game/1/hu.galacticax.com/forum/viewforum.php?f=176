    //document.domain = "galacticax.com";
    function setFrameHeight(){
        window.parent.setForumFrameDef();
        var height = $(document).height();
        window.parent.setForumFrame(height);
    }

    $(document).ready(function(){
        if (typeof window.parent.setForumFrameDef == 'function' &&
            typeof window.parent.setForumFrame == 'function'){
                setFrameHeight();
        }
    });
</script>

