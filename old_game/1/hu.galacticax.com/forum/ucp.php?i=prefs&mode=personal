<script type="text/javascript">
$(document).ready(function(){
	$("#fTabs #ftTop li").hover(function(){
		$( this ).each(function( c, e ){
			if( $(this).hasClass('active') ){ return; }
			$( this ).addClass('hover');
		});
	}, function(){
		$( this ).each(function( c, e ){
			$( this ).removeClass('hover');
		});
	});
});
</script>
